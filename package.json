    "stats:create": "cross-env NODE_ENV=production BABEL_ENV=production webpack --config webpack.config.js --profile --json > dist/stats.json",
    "stats:analyze": "webpack-bundle-analyzer -s gzip dist/stats.json",
    "scss-utils": "scss src/scss/utils/variables/_index.scss ./scss-utils.css"
